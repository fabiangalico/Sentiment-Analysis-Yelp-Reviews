Sys.setenv(JAVA_HOME="C:/Program Files/Java/jdk-11.0.2/")
library(rJava)
library(tm)
library(RWeka)
tok <- function(x) NGramTokenizer(x, Weka_control(min=2, max=2))
tok("rewarding hard work with raising taxes and VAT. #LabourManifesto")
